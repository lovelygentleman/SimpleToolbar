<Application x:Class="Toolbar.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Toolbar"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/WebsiteStyle.xaml"/>
                <ResourceDictionary Source="/Style/MuteStyle.xaml"/>
                <ResourceDictionary Source="/Style/UnmuteStyle.xaml"/>
                <ResourceDictionary Source="/Style/NoteStyle.xaml"/>
                <ResourceDictionary Source="/Style/FlightStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
